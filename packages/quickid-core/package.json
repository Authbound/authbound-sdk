{
  "name": "@authbound/quickid-core",
  "version": "0.1.0",
  "description": "Authbound QuickID core SDK: framework-agnostic client for document + selfie verification.",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "author": "Authbound",
  "homepage": "https://github.com/authbound/sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/authbound/sdk.git",
    "directory": "packages/quickid-core"
  },
  "bugs": {
    "url": "https://github.com/authbound/sdk/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@authbound/core": "workspace:*",
    "@authbound/shared": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@authbound/tsconfig": "workspace:*",
    "openapi-typescript-codegen": "^0.29.0",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
